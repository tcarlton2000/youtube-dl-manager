{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver",
      {
        "root": ["./app/static/js"],
        "alias": {
          "Components": "./app/static/js/components"
        }
      }
    ]
  ]
}
